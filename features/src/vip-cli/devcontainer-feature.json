{
    "id": "vip-cli",
    "name": "VIP CLI",
    "version": "1.2.0",
    "description": "Installs VIP CLI into the Dev Environment",
    "options": {
        "enabled": {
            "type": "boolean",
            "description": "Enable VIP CLI",
            "default": true
        },
        "version": {
            "type": "string",
            "description": "Which version of VIP CLI to install",
            "default": "latest",
            "proposals": ["latest", "next"]
        },
        "import_db": {
            "type": "boolean",
            "description": "Import the database from the VIP Go environment",
            "default": false
        }
    }
}
